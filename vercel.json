{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "routes": [
    { "src": "/api/projects/(?<id>[^/]+)", "dest": "/api/projects_[id].js" },
    { "src": "/api/projects", "dest": "/api/projects.js" },
    { "src": "^(?!/api).*", "dest": "/index.html" }
  ],
  "env": {
    "VITE_DEFAULT_PROJECT_ID": "demo"
  }
}


